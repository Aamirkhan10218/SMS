@*@model SMS.Areas.Dashboard.ViewModels.TeacherListingModel*@
@{
    ViewBag.Title = "Index";
}
<div class="row">
    <div class="col-lg-12">
        <h2>Teacher ()</h2>
        <hr />
    </div>
</div>
<div class="row mb-3">
    <div class="col-lg-12">
        <div class="input-group">
            <input type="text" class="form-control" placeholder="Search Any thing here" aria-label="Recipient's username with two button addons" aria-describedby="button-addon4">
            <div class="input-group-append" id="button-addon4">
                <button class="btn btn-outline-success" type="button"><i class="fas fa-search mr-1"></i>Search</button>
                <button class="btn btn-outline-secondary" type="button"><i class="fas fa-retweet mr-1"></i>Reset</button>
                <button id="btnCreate" class="btn-outline-primary" type="button" data-toggle="modal" data-target="#exampleModal"><i class="far fa-plus-square mr-1"></i>Create</button>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        @{ Html.RenderAction("Listing", "Teachers");}
    </div>
</div>
<!--MODAL-->
<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" >
    <div class="modal-dialog" role="document">
      
    </div>
</div>
<!--SCRIPT FOR OPENING MODAL-->
<script>
    $("#btnCreate").click(function () {
        $.ajax({
            url: '@Url.Action("Create","Teachers")',
        })
            .done(function (response) {

                $("#exampleModal .modal-dialog").html(response);
            });
    });
</script>
